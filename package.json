{
    "name": "calix-axos",
    "displayName": "Calix AXOS Syntax",
    "description": "Calix AXOS Syntax Highlighter",
    "version": "1.0.0",
    "publisher": "tjdub",
    "icon": "images/calix.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tjdub/vscode-calix-axos-syntax.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "calix-axos",
                "aliases": [
                    "axos",
                    "calix-axos"
                ],
                "extensions": [
                    ".axos"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "snippets": [
            {
                "language": "calix-axos",
                "path": "./snippets/snippets.axos.json"
            }
        ],
        "grammars": [
            {
                "language": "calix-axos",
                "scopeName": "text.axos",
                "path": "./syntaxes/axos.tmGrammar.repo.json"
            }
        ]
    }
}
